{
  "name": "cpu-features",
  "version": "0.0.2",
  "author": "Brian White <mscdex@mscdex.net>",
  "description": "A simple binding to Google's cpu_features library for obtaining information about installed CPU(s)",
  "main": "./lib/index",
  "dependencies": {
    "install-addons": "mscdex/install-addons#master"
  },
  "devDependencies": {
    "@mscdex/eslint-config": "^1.1.0",
    "eslint": "^7.0.0"
  },
  "scripts": {
    "install": "node-install-addons",
    "test": "node test/test.js",
    "lint": "eslint --cache --report-unused-disable-directives --ext=.js .eslintrc.js lib test",
    "lint:fix": "npm run lint -- --fix"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "cpu",
    "detect",
    "detection",
    "features"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mscdex/cpu-features/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mscdex/cpu-features.git"
  },
  "install-addons": {
    "buildDependencies": {
      "buildcheck": "0.0.3",
      "nan": "^2.15.0"
    },
    "preBuildCommand": "node buildcheck.js > buildcheck.gypi",
    "buildCommand": "node-gyp rebuild",

    "usesNapi": false,

    "binaryRepo": "mscdex/cpu-features",
    "binaryOutPath": "build/Release/cpufeatures.node"
  }
}
